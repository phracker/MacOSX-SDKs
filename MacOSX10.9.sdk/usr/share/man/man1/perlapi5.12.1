and \f(CW\*(C`sv_mortalcopy\*(C'\fR.
.Sp
.Vb 1
\&        SV*     sv_2mortal(SV *const sv)
.Ve
.IP "sv_2nv" 8
.IX Xref "sv_2nv"
.IX Item "sv_2nv"
