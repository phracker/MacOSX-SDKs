\&    $port = "someother.com:20203/ssleay";
\&    $def_host  = "default\-domain.com";
\&    $def_proto = "tcp";
\&    $def_ipv   = undef;
\&    @info = Net::Server::Proto\->parse_info($port,$def_host,$def_proto,$def_ipv);
