framework module CoreTelephony [extern_c] {
    // Add " requires !swift" to remove support for Swift
    umbrella "Headers"

    export *
    module * { export * }

    exclude header "CoreTelephonyDefines.h"
    exclude header "CTTelephonyNetworkInfo.h"
    exclude header "CTSubscriberInfo.h"
    exclude header "CTSubscriber.h"
    exclude header "CTCarrier.h"
    exclude header "CTCallCenter.h"
    exclude header "CTCall.h"
}
