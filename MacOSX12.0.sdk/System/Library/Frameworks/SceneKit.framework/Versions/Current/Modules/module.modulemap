framework module SceneKit [system] {
  umbrella header "SceneKit.h"
  
  export *
  module * { export * }
  
  // The contents of explicit submodules are only made available when the submodule itself was explicitly named in an import declaration or was re-exported from an imported module.
  explicit module ModelIO {
    header "ModelIO.h"
    export *
  }
}
